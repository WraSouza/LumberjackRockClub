<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="LumberjackRockClub.View.FacebookView"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Facebook"
    NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <WebView
                x:Name="navegador"
                Navigated="paginaCarregada"
                Navigating="carregandoPagina"
                VerticalOptions="FillAndExpand" />
            <Label x:Name="lblStatus" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>